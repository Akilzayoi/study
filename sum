#include <iostream>

using namespace std;

int main()
{
    int num1, num2, sum;

    // 获取用户输入
    cout << "Enter the first number: ";
    cin >> num1;
    cout << "Enter the second number: ";
    cin >> num2;

    // 计算和
    sum = num1 + num2;

    // 打印结果
    cout << "The sum of " << num1 << " and " << num2 << " is " << sum << endl;

    return 0;
}
